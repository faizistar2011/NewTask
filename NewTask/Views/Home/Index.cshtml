@model List<AppUser>

@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-12">
        <table class="table">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th>Email</th>
                    <th>Role</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                        <td>@item.UserName</td>
                        <td>@item.Email</td>
                        <td>@item.Role</td>
                    </tr>
                    
                }
               
            </tbody>
        </table>
    </div>
</div>
